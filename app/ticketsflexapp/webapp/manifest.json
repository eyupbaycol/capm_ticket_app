{
    "_version": "1.49.0",
    "sap.app": {
        "id": "com.fe.ticketsflexapp",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:fpm",
            "version": "1.10.5",
            "toolsId": "5636a8b9-b2fa-4d9f-a1d8-a4d0decce65d"
        },
        "dataSources": {
            "mainService": {
                "uri": "/ticketservice/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "4.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "dependencies": {
            "minUI5Version": "1.117.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.ushell": {},
                "sap.fe.templates": {},
                "sap.f": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.fe.ticketsflexapp.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            }
        },
        "resources": {
            "css": []
        },
        "routing": {
            "routes": [
                {
                    "name": "TicketMain",
                    "pattern": ":?query:",
                    "target": [
                        "TicketMain"
                    ]
                },
                {
                    "name": "TicketObjectPage",
                    "pattern": "Ticket({TicketKey}):?query:",
                    "target": [
                        "TicketMain",
                        "TicketObjectPage"
                    ]
                }
            ],
            "targets": {
                "TicketMain": {
                    "type": "Component",
                    "id": "TicketMain",
                    "name": "sap.fe.core.fpm",
                    "options": {
                        "settings": {
                            "viewName": "com.fe.ticketsflexapp.ext.main.Main",
                            "contextPath": "/Ticket",
                            "navigation": {
                                "Ticket": {
                                    "detail": {
                                        "route": "TicketObjectPage"
                                    }
                                }
                            }
                        }
                    },
                    "controlAggregation": "beginColumnPages",
                    "contextPattern": ""
                },
                "TicketObjectPage": {
                    "type": "Component",
                    "id": "TicketObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "navigation": {},
                            "contextPath": "/Ticket",
                            "content": {
                                "body": {
                                    "sections": {
                                        "CustomerLocation": {
                                            "template": "com.fe.ticketsflexapp.ext.fragment.CustomerLocation",
                                            "position": {
                                                "placement": "After",
                                                "anchor": "TicketPersonelleri"
                                            },
                                            "title": "Lokasyon Bilgisi",
                                            "type": "XMLFragment"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "controlAggregation": "midColumnPages",
                    "contextPattern": "/Ticket({TicketKey})"
                }
            },
            "config": {
                "flexibleColumnLayout": {
                    "defaultTwoColumnLayoutType": "TwoColumnsMidExpanded",
                    "defaultThreeColumnLayoutType": "ThreeColumnsEndExpanded"
                },
                "routerClass": "sap.f.routing.Router"
            }
        },
        "extends": {
            "extensions": {
                "sap.ui.controllerExtensions": {
                    "sap.fe.templates.ObjectPage.ObjectPageController#com.fe.ticketsflexapp::TicketObjectPage": {
                        "controllerName": "com.fe.ticketsflexapp.ext.controller.CustomControllerExt"
                    }
                }
            }
        },
        "rootView": {
            "viewName": "sap.fe.templates.RootContainer.view.Fcl",
            "type": "XML",
            "async": true,
            "id": "appRootView"
        },
        "flexEnabled": true
    }
}